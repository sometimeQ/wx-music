/* pages/ranking-detail/index.wxss */

.nav-bar {
  position: fixed;
  /* left: 0; */
  top: 0;
  width: 100%;
  z-index: 99;
  background: green;
}

/* 胶囊按钮区域 */
.nav-bar .menu-bar {
  /* background: pink; */
  display: flex;
  align-items: center;

  /* 左边 */
  position: absolute;
  color: #fff;
  width: 100%;
  font-size: 20px;
  font-weight: 700;
}

/* 导航栏左边按钮 */
.nav-bar .menu-bar .left {
  display: flex;
}

.nav-bar .menu-bar .arrow {
  height: 40rpx;
  width: 40rpx;
}

/* 标题 */
.nav-bar .menu-bar .title {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
  position: absolute;
  left: 0;
  right: 0;
  margin: 0 auto;
}

/* .ranking-header {
  position: fixed;
  top: 0;
} */

/* 榜单列表 */
.rankingList {
  z-index: 1;
  margin: 20rpx 24rpx 20rpx 24rpx;
}